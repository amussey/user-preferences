<?xml version="1.0"?>
<root>
  <list>
    <item>
      <name>Print Screen bound to take a Screen Shot</name>
      <identifier>private.printscreen.screenshot</identifier>
      <autogen>
        __KeyToKey__
        KeyCode::F13,
        KeyCode::KEY_4, ModifierFlag::COMMAND_L | ModifierFlag::SHIFT_L
      </autogen>
    </item>
  </list>
  <appdef>
    <appname>TEAMVIEWER</appname>
    <equal>com.TeamViewer.TeamViewer</equal>
    <equal>com.teamviewer.TeamViewer</equal>
  </appdef>

  <item>
    <name>Disable all settings while you are using TeamViewer.</name>
    <identifier>private.ignore_teamviewer</identifier>
    <only>TEAMVIEWER</only>
    <autogen>__PassThrough__</autogen>
  </item>
</root>
